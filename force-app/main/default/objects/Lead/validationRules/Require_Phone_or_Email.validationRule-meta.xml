<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Require_Phone_or_Email</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Email), ISBLANK(Phone))</errorConditionFormula>
    <errorMessage>Please provide either phone number or email for as contact.</errorMessage>
</ValidationRule>
